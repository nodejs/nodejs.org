{
  "name": "@node-core/rehype-shiki",
  "version": "1.2.0",
  "type": "module",
  "exports": {
    ".": "./src/index.mjs",
    "./index.css": "./src/index.css",
    "./*": "./src/*.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejs/nodejs.org",
    "directory": "packages/rehype-shiki"
  },
  "scripts": {
    "lint": "node --run lint:js",
    "lint:fix": "node --run lint:js:fix",
