    "@shikijs/twoslash": "^3.13.0",
    "classnames": "catalog:",
    "hast-util-to-string": "^3.0.1",
    "shiki": "~3.13.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "catalog:"
  }
