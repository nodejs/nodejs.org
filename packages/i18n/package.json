{
  "name": "@node-core/website-i18n",
  "type": "module",
  "exports": {
    "./*": [
      "./src/*",
      "./src/*.d.ts",
      "./src/*.mjs",
      "./src/*.json"
    ],
    ".": "./src/index.mjs"
  },
  "main": "./src/index.mjs",
  "module": "./src/index.mjs",
  "scripts": {
    "lint": "turbo lint:js",
    "lint:js": "eslint \"**/*.{js,mjs,ts}\"",
    "lint:fix": "node --run lint -- -- --fix"
  },
  "devDependencies": {
    "eslint-import-resolver-typescript": "~4.4.4",
    "typescript": "catalog:",
    "typescript-eslint": "~8.38.0"
  }
}
